<%= form_for(list) do |f| %>

<% if list.errors.any? %>
    <div id="error_explanation">
      <ul>
        
        <% list.errors.full_lists.each do |list| %>
          <li>
             <%= list %>
          </li>
        <% end %>

      </ul>
    </div>
<% end %>
  
<%= f.label :status, 'タイトル' %>
<%= f.text_field :status %>  
  
<%= f.label :content, 'メッセージ' %>
<%= f.text_field :content %>


  
<%= f.submit '投稿' %>

<% end %>